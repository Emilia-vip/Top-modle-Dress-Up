{
  "compilerOptions": {
    "jsx": "react-jsx",          // <- Viktig! TypeScript förstår JSX nu
    "esModuleInterop": true,
    "module": "ESNext",
    "target": "ES2020",
    "moduleResolution": "Node",
    "allowJs": true,
    "strict": true,
    "forceConsistentCasingInFileNames": true
  },
  "include": ["src/**/*"]
}
